Questions 1.
import java.io.*;
import java.util.*;
import java.util.Scanner;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int[] arr = new int[n];
        for (int m = 0; m < n; m++) {
        arr[m] = sc.nextInt();
        }
        sc.close();
        int sum = 0;
        for (int m = 0; m < n; m++) {
            sum += arr[m];
        }System.out.println(sum);
    }
}

Questions 2.

import java.io.*;
import java.util.*;
import java.util.Scanner;
public class Solution {
 public static void main(String[] args) {
 Scanner xy = new Scanner(System.in);
 String day = xy.nextLine();
 xy.close();
 String openTime, closeTime;
switch(day) {
case "Monday":
case "Tuesday":
 case "Wednesday":
case "Thursday":
 case "Friday":
 openTime = "8:30";
 closeTime = "5:30";
break;
case "Saturday":
 openTime = "9:30";
 closeTime = "4:30";
 break;
default:
System.out.println("Library is closed");
return;
 }
 System.out.println(openTime + " " + closeTime);
 }
}
